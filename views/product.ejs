    let d=c[3]
    d.addEventListener("click",function(){
        document.getElementById("val").textContent=d.value
        if(d.value=="Price : High to Low") {
            document.getElementById("productsContDk").innerHTML=null;
            product_data.sort(function(a,b) {return b.newprice.$numberInt-a.newprice.$numberInt})
            displayProducts(product_data)
        }
        else if(d.value == "Price : Low to High") {
            document.getElementById("productsContDk").innerHTML=null;
            product_data.sort(function(a,b) {return a.newprice.$numberInt-b.newprice.$numberInt})
            displayProducts(product_data)
        }
        else if(d.value == "Coustmer Top Rated") {
            document.getElementById("productsContDk").innerHTML=null;
            product_data.sort(function(a,b) {return b.ratings.$numberDouble-a.ratings.$numberDouble})
            displayProducts(product_data)
        }
        else if(d.value == "Discount") {
            document.getElementById("productsContDk").innerHTML=null;
            product_data.sort(function(a,b) {
                // console.log(a.discount.split(" "))
                let a1=a.discount.split(" ")
                let a2=a1[0];
                // console.log(a2)
                let a3=a2.split("")
                
                a3.pop()
                // console.log(a3)
                let a4=a3.join("")

                let b1=b.discount.split(" ")
                let b2=b1[0];
                let b3=b2.split("")
                b3.pop()
                let b4=b3.join("")
                console.log(b,a)
                return b4-a4
                
           
            })
            displayProducts(product_data)
        }

        else if(d.value == "Name") {
            document.getElementById("productsContDk").innerHTML=null;
            product_data.sort(function(a,b) {
                if(a.name[0]>b.name[0]) return 1
                if(a.name[0]<b.name[0]) return -1
            })
            displayProducts(product_data)
        }
        else {
            document.getElementById("productsContDk").innerHTML=null;
            product_data.sort(function(a,b) {
                if(a.name[0]>b.name[0]) return -1
                if(a.name[0]<b.name[0]) return 1
            })
            displayProducts(product_data)
        }
    })
}
